<Window x:Class="GameShop.Win.SomeAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameShop.Win"
        mc:Ignorable="d" Style="{StaticResource ggg}"
         xmlns:rs="clr-namespace:GameShop.Languages"
        Title="" Height="155" MinHeight="150" MinWidth="220" Width="226">
    <StackPanel HorizontalAlignment="Left">
        <Label Margin="5"  Content="{x:Static rs:Language.AddText}"></Label>
        <TextBox x:Name="textBox1" Width="150" Margin="5"></TextBox>
        <StackPanel Orientation="Horizontal" Margin="5">
            <Button Margin="5" Content="{x:Static rs:Language.AddText}" Click="Button_Click"></Button>
            <Button Margin="5" Content="{x:Static rs:Language.CancelText}" Click="Button_Click_1"></Button>
        </StackPanel>
    </StackPanel>
</Window>
